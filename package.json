  "bugs": {
    "url": "https://github.com/bluesalt-labs/ledj/issues"
  },
  "homepage": "http://ledj.bluesaltlabs.com/",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "css-loader": "^0.28.4",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0"
  }
}
